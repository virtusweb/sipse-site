#tit
  Contacto

#dir
  Calle Jorge Jiménez Cantú Mz.6 Lote 1-A<br/>	
  Col. Emiliano Zapata Atizapán de Zaragoza C.P. 52918<br/>
  Estado de México<br/>


  #hojac
    %p Si deseas contactarte SIPSE, S.A. de C.V. para solicitar información de nuestros servicios, llena el siguiente formato.
    %form{:action => "contacto", :method => "post"}
      %label{:for => "empresa"} Empresa:
      %br
      %input{:name => "empresa", :type => "text", :size => "30"}
      %br
      %label{:for => "colonia"} Colonia:
      %br
      %input{:name => "colonia", :type => "text", :size => "30"}
      %br
      %label{:for => "calle"} Calle y No.:
      %br
      %input{:name => "calle", :type => "text", :size => "30"}
      %br
      %label{:for => "cp"} C.P.:
      %br
      %input{:name => "cp", :type => "text", :size => "30"}
      %br
      %label{:for => "ciudad"} Ciudad:
      %br
      %input{:name => "ciudad", :type => "text", :size => "30"}
      %br
      %label{:for => "nombre"} Nombre:
      %br
      %input{:name => "nombre", :type => "text", :size => "30"}
      %br
      %label{:for => "puesto"} Puesto:
      %br
      %input{:name => "puesto", :type => "text", :size => "30"}
      %br
      %label{:for => "telefono"} Teléfono:
      %br
      %input{:name => "telefono", :type => "text", :size => "30"}
      %br
      %label{:for => "email"} Email:
      %br
      %input{:name => "email", :type => "text", :size => "30"}
      %br
      %label{:for => "to"} Mensaje:
      %br
      %textarea{:cols => "40", :name => "message", :rows => "10"}
      %br
      %input{:type => "submit", :value => "Enviar"} 

